	import com.x.saleschannel.v1.*
	import com.x.marketplace.v1.*
	
    contract Marketplace 
    {
    	description "An adapter to a sales channel that is an online marketplace, such as eBay"
    	
    	roles
    	{
    		//sales channel roles 
	   		SalesChannelSetup.SalesChannelMetadataProvider
