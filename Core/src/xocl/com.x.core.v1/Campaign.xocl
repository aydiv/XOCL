
package com.x.core.v1
{
	enum CampaignStatus
	{
		DRAFT,
		SCHEDULED,
		EXECUTING,
		COMPLETED
	}

	record Campaign
	{
		string? id

		/** The account this campaign belongs to */
		string accountId
		
		string name
		string? _avdl_description

		/** Date Time Format: ISO 8601 */
		 datetime? startTime
		 datetime? endTime
		
		/** Provider specific types can be specified as strings */
		union{CampaignStatus,string} status
	}	
}
